{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "https://example.com/employee.schema.json",
    "title": "Module Entity",
    "description": "This document defines module entity",
    "type": "object",
    "properties": {
        "id": {
            "description": "A unique identifier for a module",
