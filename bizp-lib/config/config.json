  "version": "2.0",
  "bundles": {
    "bizpcomps-library": {
      "components": [
        {
          "entrypoint": "./lib/index.js",
          "manifest": "./src/libraries/bizpcomps/BizpcompsLibrary.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BizpcompsLibraryStrings": "lib/libraries/bizpcomps/loc/{locale}.js",
